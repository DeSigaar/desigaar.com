		"prettier": "^2.2.1",
		"redux-devtools-extension": "^2.13.9",
		"sort-package-json": "^1.49.0",
		"typescript": "^4.2.3"
	},
	"engines": {
		"node": ">= 8.17.0",
		"npm": ">= 6.12.1",
		"yarn": ">= 1.19.1"
	}
